{
  "name": "react-webpack-seed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --updateSnapshot",
    "build": "webpack --env production",
    "dev": "webpack --env development",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development --open\""
  },
  "repository": "git+https://github.com/AJarombek/react-webpack-seed.git",
  "keywords": [],
  "author": "Andrew Jarombek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJarombek/react-webpack-seed/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setupTests.js",
    "moduleNameMapper": {
      "\\.(scss)$": "jest-css-modules",
      "\\.(png|ttf)$": "../mocks/fileMock.js"
    },
    "collectCoverageFrom": [
      "src/**"
    ],
    "coveragePathIgnorePatterns": [
      "./src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "homepage": "https://github.com/AJarombek/react-webpack-seed#readme",
  "dependencies": {
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "webpack": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  }
}
