{
  "name": "react-webpack-seed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --updateSnapshot",
    "build": "webpack --env production",
    "dev": "webpack --env development",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development --open\"",
    "lint": "eslint src",
    "prettier:check": "prettier src --check",
    "prettier:write": "prettier src --write"
  },
  "repository": "git+https://github.com/AJarombek/react-webpack-seed.git",
  "keywords": [],
  "author": "Andrew Jarombek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJarombek/react-webpack-seed/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setupTests.js",
    "moduleNameMapper": {
      "\\.(scss)$": "jest-css-modules",
      "\\.(png|ttf)$": "../mocks/fileMock.js"
    },
    "collectCoverageFrom": [
      "src/**"
    ],
    "coveragePathIgnorePatterns": [
      "./src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "homepage": "https://github.com/AJarombek/react-webpack-seed#readme",
  "dependencies": {
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.1",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-css-modules": "^2.1.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
